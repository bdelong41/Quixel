<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="150" prefWidth="350" styleClass="anchor-pane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.inventorysystem.controller.CloseProgram">
   <children>
      <Button fx:id="close_save_button" layoutX="52.0" layoutY="101.0" mnemonicParsing="false" onAction="#apply_button_listener" text="apply" />
      <Button fx:id="dont_apply_button" layoutX="137.0" layoutY="101.0" mnemonicParsing="false" onAction="#dontApply_button_listener" text="don't apply" />
      <Button fx:id="closeCancelButton" layoutX="256.0" layoutY="101.0" mnemonicParsing="false" onAction="#cancel_button_listener" text="cancel" />
      <Label layoutX="93.0" layoutY="58.0" text="Do you wish to apply changes?" />
   </children>
</AnchorPane>
